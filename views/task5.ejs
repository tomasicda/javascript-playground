<script type="text/javascript">

//Loop that iterates up to 100 while outputting 'fizz' at muliples of 3 ..... blah blah

(function (){
  for (var i = 0; i < 100; i++) {
    var output = " "
      if (i % 3 === 0) {output += " Fizz"}
      if (i % 5 === 0) {output += " Buzz"}
      if (i % 3 === 0 & i % 5 === 0) {output += " FizzBuzz"}
      console.log(output, " -- " , i);
  }
})();

</script>

